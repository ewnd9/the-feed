  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run build:backend && npm run build:frontend && cp dist/*css* public/",
    "build:backend": "NODE_ENV=production webpack --config webpack.config.backend.js",
    "build:frontend": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "clean": "rm -rf dist && rm -rf public",
    "lint": "eslint 'src/**/*.js' 'frontend/**/*.js' 'test/**/*.js'",
    "prepush": "npm run lint && npm test",
    "start": "node dist/backend",
    "start:dev": "CONFIG=config-dev.yml babel-watch --exclude src/app src/server",
    "test": "nyc --require babel-register --all --reporter=lcov --reporter=text ava",
    "test:tasks": "ava src/tasks/**/*.spec.js",
    "test:watch": "nodemon test/*.js --exec ava -- --serial --verbose"
  },
  "private": "true",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ewnd9/the-feed.git"
  },
  "keywords": [
    "INSERT_KEYWORDS"
  ],
  "author": "ewnd9 <ewndnine@gmail.com>",
  "license": "MIT",
  "nyc": {
    "exclude": [
      "public/",
      "dist/"
    ],
    "include": [
      "src/**/*.js"
    ]
  },
