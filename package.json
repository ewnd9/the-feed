{
  "name": "the-feed",
  "version": "0.0.0",
  "description": "the-feed web app",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run build:backend && npm run build:frontend && cp dist/*css* public/",
    "build:backend": "NODE_ENV=production node_modules/webpack/bin/webpack.js --config webpack.config.backend.js",
    "build:frontend": "NODE_ENV=production node_modules/webpack/bin/webpack.js --config webpack.config.prod.js",
    "clean": "rm -rf dist && rm -rf public",
    "lint": "eslint 'src/**/*.js' 'frontend/**/*.js' 'test/**/*.js'",
    "precommit": "npm run lint && npm test",
    "start": "node app",
    "start:dev": "NODE_ENV=development DB_PATH=db/ PORT=3000 babel-watch app-dev",
    "test": "ava test",
    "test:cov": "nyc --reporter=lcov --reporter=text npm test",
    "test:tasks": "ava src/tasks/**/*.spec.js",
    "test:watch": "npm run test -- --watch"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "private": "true",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ewnd9/the-feed.git"
  },
  "keywords": [
    "INSERT_KEYWORDS"
  ],
  "author": "ewnd9 <ewndnine@gmail.com>",
  "license": "MIT",
